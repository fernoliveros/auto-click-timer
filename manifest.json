{
  "name": "Auto Click Timer",
  "description": "clicks automatically on a timer",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "activeTab", "scripting", "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/mouse_icon.png",
      "32": "/images/mouse_icon.png",
      "48": "/images/mouse_icon.png",
      "128": "/images/mouse_icon.png"
    }
  }
}